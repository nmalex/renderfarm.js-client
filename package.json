  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@types/node": "^15.0.1",
    "babel-loader": "^8.2.2",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.13.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.9.7",
    "uglify-js": "^3.14.2",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
