  "description": "A nice greeter",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "webpack": "webpack",
    "uglify": "uglifyjs --compress --mangle --output ./dist/index.min.js ./dist/index_bundle.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
